function main() {
    int n = 10; // how many Fibonacci numbers to print
    int a = 0;
    int b = 1;

    print(a);
    print(b);

    int i = 2;
    while (i < n) {
        int c = a + b; // F(i) = F(i-1) + F(i-2)
        print(c);
        a = b;
        b = c;
        i = i + 1;
    }
    return 0;
}

main();