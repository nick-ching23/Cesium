Stream s;
Reactive r = s * 2 + 1;

function setAndShow(int x) {
    setValue(s, x);
    print(r);  // prints s*2+1
    return 0;  // Functions must return int, so return 0 as a dummy value
}

// Initially s=null, so r=null
print(r); // Expect "null"

setAndShow(5);  // s=5 => r=5*2+1=11
setAndShow(10); // s=10 => r=21

// After function calls, s=10
print(r); // Should still be 21